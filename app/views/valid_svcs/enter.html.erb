<% provide(:title, 'Enter your SVC') %>
<h1>Enter your SVC</h1>

<%= form_for :valid_svc, url: confirm_election_valid_svcs_path(@election), html: { autocomplete: :off } do |f| %>

	<%= f.text_field :svc, placeholder: "SVC" %>

	<%= f.submit "Enter", class: "btn btn-large btn-primary btn-block" %>
<% end %>

